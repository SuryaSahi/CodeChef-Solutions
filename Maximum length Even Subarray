for i in range(int(input())):
        n = int(input())
        sum = 0
        count = 0
        for i in range(1,n+1):
                sum +=i
                count +=1
        while count > 0:
                if sum % 2 == 0:
                        print(count)
                        break
                else:
                        sum = sum - n
                        n -=1
                        count -= 1
                
        
                
